{
	"name" : "chrisgruen/ajaxselectlist",
	"type" : "typo3-cms-extension",
	"description" : "Test ajax",
	"homepage" : "https://github.com/chrisgruen/realty-manager",
	"license" : "GPL-2.0-or-later",
	"minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Christian Gruenwald",
            "email": "cg@romonta-schach.de",
            "homepage": "https://demochris.de/",
            "role": "developer"
        }
    ],
	"keywords" : [
		"TYPO3 CMS"
	],
	"require" : {
		"bk2k/bootstrap-package" : "^11.0"
	},
	"autoload" : {
		"psr-4" : {
			"Sebkln\\Ajaxselectlist\\": "Classes/"
		}
	},
	"config": {
        "optimize-autoloader": true,
        "vendor-dir": ".build/vendor",
        "bin-dir": ".build/bin"
    },
    "extra": {
		"branch-alias": {
			"dev-master": "10.x-dev"
		},
		"typo3/cms": {
			"extension-key": "ajaxselectlist",
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	}
}